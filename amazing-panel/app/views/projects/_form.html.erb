<%= form_for(@project, :html => {:multipart => true}) do |f| %>
  <%= render 'shared/error_messages', :object => f.object %>

  <div class="field">
    <%= label_tag :logo %><br />
    <%= file_field_tag :file %>
  </div>
  <div class="field">
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </div>
  <div class="field">
    <%= f.label :private %>
    <%= f.check_box :private %>
  </div>
  <div class="field">
    <%= f.label :description %><br />
    <%= f.text_area :description, :size => "60x20" %>
  </div>

  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
