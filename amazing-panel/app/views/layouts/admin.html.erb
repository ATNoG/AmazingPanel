<% content_for :main_content do %> 
  <div id="toolbar">
    <ul id="toolbar-action-list">
        <%= add_action 'Users', admin_users_path, ['admin/users'] %>
	<%= add_action 'Baseline Images', admin_sys_images_path, ['admin/sys_images']%>
	<%= add_action 'Testbeds', admin_testbeds_path, ['admin/testbeds']%>
    </ul>
  </div>
  <div id="sub-toolbar">
    <%= yield :sub_actions %>
  </div>
  <br/>
  <div id="file-show">
    <%= yield :content %>
  </div>
  <div class="clear"></div>
<% end %>

<%= render :template => 'layouts/application' %>
