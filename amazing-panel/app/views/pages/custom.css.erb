  padding:2px;  
}


input[type=submit] {	
  border:1px solid #bbb;  
  border-radius:5px;  
  -moz-border-radius:5px;  
  -webkit-border-radius:5px;  
  padding:5px; 
  cursor: pointer; 
  color: #111;
