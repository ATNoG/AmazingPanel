/** Modal Dialog From Scratch 
  * Filters
  */
.overlay {
  display: none;
  position:absolute;
  width: 100%;
  height: 100%;
  background-color:#000;  
  opacity: 0.6;
  filter: alpha(opacity=60);  
  z-index: 1;
  left: 0;
  top: 0;
}

.modal {
  display: none;
  position: absolute;
  height: 100%;
  top: 30%;
  left: 40%; 
  z-index: 2;
  width: 300px;
  height: 300px;
  background-color: rgba(82, 82, 82, 0.699219);
  padding:10px;
  /*padding-left: 5px;*/
  -moz-border-radius: 8px;
  -webkit-border-radius: 8px;
  border-radius: 8px;  
}

.modal-container {
  background-color: white;
  height: 263px;
  overflow: auto;
}

.modal-actions {  
  padding-top: 10px;
  padding-right: 10px;
  background-color: #f0f0f0;
  cursor: pointer;
  height: 30px;
  border: 1px solid #d1d1d1;
}

.button {
  padding: 2px;
  background: #148FC9;
  height: 18px;
  width: 50px;
  color: white !important; 
  border: 1px solid #107BAE;
  text-align:center;
  float:right;
  cursor: pointer;
  font-weight: bold;
}

.button a {
  color: white;
}

.close-button {
  background: #EE1111;
  border: 1px solid #DD5555;
  font-size: 10px;
  position: absolute;
  right: 15px;
  top: 15px;
  width: 18px;
  line-height: 18px;
}

.check-button {
  background: #7D1;
  border: 1px solid #1D5;
  font-size: 10px;
  position: absolute;
  right: 45px;
  top: 15px;
  width: 18px;
  line-height: 18px;
}

.dialog-active {
  display: block;
}

.title-box {
  font-weight: bold;
  font-size: 14px;
  text-decoration: uppercase;
