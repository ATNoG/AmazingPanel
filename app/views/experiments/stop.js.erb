$("#run-experiment-button").removeClass("button-disabled");
$("#stop-experiment-button").addClass("button-disabled");
<% if @error.nil? %>
$("#error-experiment").addClass("hidden");
<% else %>
$("#error-experiment").removeClass("hidden");
$("#error-experiment > .text").html("<%= @error %>");
<% end %>
